{
  email tareksnews@gmail.com
}

cv.kroete.io {
  encode zstd gzip

  reverse_proxy app:8000 {
    transport http {
      read_timeout 120s
      write_timeout 120s
      dial_timeout 10s
    }
  }

  header {
    X-Content-Type-Options "nosniff"
    X-Frame-Options "DENY"
    Referrer-Policy "no-referrer"
    Permissions-Policy "geolocation=(), microphone=(), camera=()"
  }
}
